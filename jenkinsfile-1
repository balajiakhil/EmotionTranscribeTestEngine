pipeline {
  agent { label 'any' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sonar-server') {
          bat 'mvn clean org.sonarsource.scanner.maven:sonar-maven-plugin:sonar'
        }
      }
    }
  }
}